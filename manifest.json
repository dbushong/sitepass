{ "name": "SitePass"
, "manifest_version": 2
, "version": "1.1"
, "description": "Generates a stable password for a each domain"
, "permissions": [ "tabs" ]
, "icons": { "16":  "icon16.png"
           , "48":  "icon48.png"
           , "128": "icon128.png"
           }
, "browser_action": { "default_icon": "icon19.png"
                    , "default_title": "Generate SitePass"
                    , "default_popup": "popup.html"
                    }
}
